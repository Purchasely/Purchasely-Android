<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:name="com.purchasely.samplev2.SampleV2Application"
        android:allowBackup="true"
        android:icon="@mipmap/logo_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Purchasely">
        <activity
            android:name="com.purchasely.samplev2.presentation.MainActivity"
            android:exported="true"
            android:theme="@style/Theme.Purchasely">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.purchasely.samplev2.presentation.screen.subscriptions.SubscriptionsActivity"
            android:exported="true"
            android:theme="@style/Theme.Purchasely">
        </activity>
    </application>

</manifest>